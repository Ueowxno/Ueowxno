      enum ICCTag {
        kTAG_rXYZ = 0;
        kTAG_gXYZ = 1;
        kTAG_bXYZ = 2;
        kTAG_rTRC = 3;
        kTAG_gTRC = 4;
        kTAG_bTRC = 5;
