const [type, setType] = useState('visa');

useEffect(() => {
    let ctype = getCardType(props.cardNumber.replaceAll(' ',''));
    ctype = ctype.length===0?'visa':ctype;
    setType(ctype.toLowerCase());
}, [props.cardNumber]);

