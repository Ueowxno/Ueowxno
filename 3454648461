$strOldSTDOUT=(posix_ttyname(STDOUT)); 

echo("This will go to the current console\r\n");
// Close the STDOUT resource 
fclose(STDOUT); 

// Reopen $STDOUT as a file Note: All further $STDOUT usage will be prefixed with a $
